{
  "name": "webp-loader",
  "version": "0.4.0",
  "description": "WebP image loader & converter for Webpack",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run test:webpack1 && npm run test:webpack2 && npm run test:webpack3 && npm run test:webpack4",
    "lint": "npm i && node_modules/.bin/eslint index.js test/*.js",
    "test:webpack1": "WEBPACK_VERSION=1.x ./scripts/test.sh",
    "test:webpack2": "WEBPACK_VERSION=2.x ./scripts/test.sh",
    "test:webpack3": "WEBPACK_VERSION=3.x ./scripts/test.sh",
    "test:webpack4": "WEBPACK_VERSION=4.x ./scripts/test.sh"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kavu/webp-loader.git"
  },
  "keywords": [
    "webpack",
    "webp",
    "image"
  ],
  "author": "Max Riveiro <kavu13@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kavu/webp-loader/issues"
  },
  "homepage": "https://github.com/kavu/webp-loader#readme",
  "dependencies": {
    "imagemin": "^5.3.1",
    "imagemin-webp": "^4.1.0",
    "loader-utils": "^1.1.0"
  },
  "devDependencies": {
    "eslint": "^4.18.2",
    "file-loader": "^1.1.11",
    "multi-loader": "^0.1.0",
    "webpack": "^1.15.0",
    "webpack-cli": "^2.0.9"
  },
  "engines": {
    "node": ">=0.6"
  },
  "peerDependencies": {
    "webpack": "*"
  }
}
